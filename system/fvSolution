FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{


    "alpha.water.*"
    {
        nAlphaCorr      5;
        nAlphaSubCycles 1;
/*        cAlpha          1;*/

        MULESCorr       yes;
		nLimiterIter    8;
        alphaApplyPrevCorr  yes;

        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-10;
        relTol          0;
        minIter         1;
    }

    "pcorr.*"
    {
        solver          PCG;
       
		preconditioner  FDIC;
	//maxIter		50;
        tolerance       1e-05;
        relTol          0;
    }

    p_rgh
    {
        solver          PCG;

        preconditioner
        {
            preconditioner  GAMG;

            smoother        GaussSeidel;

            tolerance       1e-7;
            relTol          0;
        };
        tolerance       1e-07;
        relTol          0.01;
        smoother        DIC;

    }

    p_rghFinal
    {
    	$p_rgh
        tolerance       2e-09;
        relTol          0;
        maxIter         20;    
	}

    "(U|k|epsilon|omega).*"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-9;
        relTol          0;
    }

}

PIMPLE
{
    momentumPredictor   yes;
    correctPhi          no;
    nOuterCorrectors    2;
    nCorrectors         3;
    nNonOrthogonalCorrectors 1;
}

relaxationFactors
{
    equations
    {
        ".*" 				1;
    }
}

cache
{
    grad(U);
}
