FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.water 0;
}

zones
{
    waterInit
    {
        type        box;
        zoneType    cell;

        box (-10 -10 0.399) (10 10 1.251);
        
        values
        {
            alpha.water 1;
        }
    }
}

extrapolatePatches
{
    "WALLS"   (alpha.water);
}

// ************************************************************************* //
