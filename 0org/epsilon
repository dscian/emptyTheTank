FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.0018698;

boundaryField
{
    WALLS
    {
        type            epsilonWallFunction;
        value           $internalField;
    }
    ATMOSPHERE
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    OUTLET
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
}


// ************************************************************************* //
